{
  "executor": "slurm",
  "default_resources": {
    "partition": "htc-el8",
    "mem_mb": 1000,
    "runtime": 10,
    "slurm_account": "mattei",
    "name": "{rule}-{wildcards}"
  },
  "restart_times": 1,
  "max_jobs_per_second": 10,
  "max_status_checks_per_second": 1,
  "local_cores": 1,
  "latency_wait": 3,
  "jobs": 500,
  "keep_going": true,
  "rerun_incomplete": true,
  "printshellcmds": true,
  "scheduler": "greedy",
  "use_conda": true,
  "verbose": false,
  "quiet": false
}
